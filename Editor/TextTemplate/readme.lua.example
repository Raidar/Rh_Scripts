Fmts.NoteRefFormat = "†%s"

local function NoteRefFmt (Cfg, Pattern) --> (string)
  local s = Cfg.Template.Result
  local sn = s:match(Pattern)
  sn = sn:gsub(".", List.NoteRefFormat)

  if sn then
    return Fmts.NoteRefFormat:format(sn)
  end

  return false
end -- NoteRefFmt

