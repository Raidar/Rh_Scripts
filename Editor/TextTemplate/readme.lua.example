local function DescSectio (Cfg, isLoc) --> (string)
  --logShow(Cfg, "Cfg")
  local s = Cfg.Template.Result
  s = isLoc and LocToDef(s) or s
  local sn, ss = s:match(Pats.SectioDesc1)
  if not sn then return false end
  if sn:len() > 1 then
    s = List.SectioName[sn] -- "Спец." раздел:
    if s then return Fmts.SectioName:format(s) end
    s = List.SecCombo[sn]; if s then return s end
  else
    s = List.SecOnlyName[sn] -- Только раздел:
    if s then return Fmts.SectioName:format(s) end
  end -- if
  sn, ss = sn:match(Pats.SectioDesc2)
  if not sn then return false end
  --far.Message(sn..'\n'..ss, Cfg.Template.Result)
  s = List.SectioName[sn]
  if not s then return false end
  -- Подраздел:
  local r = List.SecSubName[ss] --or SecSubName[ss:lower()]
  if r then return Fmts.SecSubName:format(s, r) end
  r = List.LatLang[ss] -- Язык раздела:
  if r then return Fmts.SectioLang:format(s, r) end
  return false
end -- DescSectio

